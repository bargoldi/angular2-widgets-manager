{
  "name": "angular2-widgets-manager",
  "version": "0.0.1",
  "description": "Angular2 dynamic components grid, based on widgets concept",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "scripts": {
    "cleanup": "rimraf src/*.ngfactory.ts src/*.ngstyle.ts src/*.ngsummary.json build dist",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "ngc": "ngc -p tsconfig.aot.json",
    "aot": "webpack --config webpack.aot.config.js -p",
    "build": "npm run cleanup && npm run ngc && npm run aot && rimraf dist/main.bundle.js dist/main.bundle.js.map",
    "build-demo": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bargoldi/ng2-widgets-manager.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "angular2",
    "widgets",
    "manager",
    "widgets-manager",
    "widgets managing",
    "grid",
    "widgets-grid",
    "ng2-widgets-grid",
    "angular2-widgets-grid"
  ],
  "author": "Bar Goldinfeld <bargoldinfeld@gmail.com> (https://github.com/bargoldi)",
  "bugs": {
    "url": "https://github.com/bargoldi/angular2-widgets-manager/issues"
  },
  "homepage": "https://github.com/bargoldi/angular2-widgets-manager",
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@angular/core": "^2.0.1"
  },
  "devDependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/compiler-cli": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "bower": "^1.7.9",
    "browserify": "^13.1.1",
    "concurrently": "^3.1.0",
